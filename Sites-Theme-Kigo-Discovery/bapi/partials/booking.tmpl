                    {{/ContextData.Quote.PaymentTerms}}
                </div>
                {{/ContextData.Quote.Statement}}
            {{/ContextData.Quote.IsValid}}
        {{/result}}

        {{#result}}
            {{#ContextData.Quote.Statement}}
            <div id="statement-details" class="modal fade" role="dialog" aria-hidden="true">
            <div class="row">
                <div class="col-md-6 col-xs-12 col-md-offset-3 bg-white m-t-15p">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                        <h3>{{textdata.Charges}}</h3>
                    </div>
                    <div class="modal-body">
                        {{#Services.length}}
                        <div id="charges-modal" class="item row-fluid">
                            <h5>{{textdata.Rent}}</h5>
                        {{/Services.length}}
                        {{#Services}}
                            {{#config.statementsettings.lumpfees}}
                                {{#config.statementsettings.lumptaxes}}
                                    <p>{{{Notes}}} <small>Incl. Fees & Taxes</small><span class="pull-right">{{#TaxesFeesAndServicesTotal}}{{TaxesFeesAndServicesTotal.LocalCurrencySymbol}}{{TaxesFeesAndServicesTotal.LocalSValue2}}{{/TaxesFeesAndServicesTotal}}{{^TaxesFeesAndServicesTotal}}--{{/TaxesFeesAndServicesTotal}}</span></p>
                                {{/config.statementsettings.lumptaxes}}
                                {{^config.statementsettings.lumptaxes}}
                                    <p>{{{Notes}}} <small>Incl. Fees</small><span class="pull-right">{{#FeesAndServicesTotal}}{{FeesAndServicesTotal.LocalCurrencySymbol}}{{FeesAndServicesTotal.LocalSValue2}}{{/FeesAndServicesTotal}}{{^FeesAndServicesTotal}}--{{/FeesAndServicesTotal}}</span></p>
                                {{/config.statementsettings.lumptaxes}}
                            {{/config.statementsettings.lumpfees}}
                            {{^config.statementsettings.lumpfees}}
                                {{#config.statementsettings.lumptaxes}}
                                    <p>{{{Notes}}} <small>Incl. Taxes</small><span class="pull-right">{{#TaxesAndServicesTotal}}{{TaxesAndServicesTotal.LocalCurrencySymbol}}{{TaxesAndServicesTotal.LocalSValue2}}{{/TaxesAndServicesTotal}}{{^TaxesAndServicesTotal}}--{{/TaxesAndServicesTotal}}</span></p>
                                {{/config.statementsettings.lumptaxes}}
                            {{/config.statementsettings.lumpfees}}
                            {{^config.statementsettings.lumpfees}}
                                {{^config.statementsettings.lumptaxes}}
                                    <p>{{{Notes}}}<span class="pull-right">{{#SubTotal}}{{SubTotal2.LocalCurrencySymbol}}{{SubTotal2.LocalSValue2}}{{/SubTotal}}{{^SubTotal}}--{{/SubTotal}}</span></p>
                                {{/config.statementsettings.lumptaxes}}
                            {{/config.statementsettings.lumpfees}}
                        {{/Services}}
                        {{#Services.length}}
                        </div>
                            <hr />
                        {{/Services.length}}
                        {{^config.statementsettings.lumpfees}}
                            {{#Fees.length}}
                            <div id="fees-modal" class="item row-fluid">
                                <h5>{{textdata.Fees}}</h5>
                            {{/Fees.length}}
                            {{#Fees}}
                                <p>{{{Notes}}} <span class="glyphicons circle_question_mark"><a data-toggle="collapse" data-target="#description{{RelatedToID}}" class="load_desc" data-entity="fee" data-related-id="{{RelatedToID}}"><i></i></a></span><span class="pull-right">{{#SubTotal}}{{SubTotal2.LocalCurrencySymbol}}{{SubTotal2.LocalSValue2}}{{/SubTotal}}{{^SubTotal}}--{{/SubTotal}}</span></p><div id="description{{RelatedToID}}" class="collapse charges-description">{{{Description}}}</div>
                            {{/Fees}}
                            {{#Fees.length}}
                            </div>
                                <hr />
                            {{/Fees.length}}
                        {{/config.statementsettings.lumpfees}}
                        {{^config.statementsettings.lumptaxes}}
                            {{#Taxes.length}}
                            <div id="taxes-modal" class="item row-fluid">
                                <h5>{{textdata.Taxes}}</h5>
                            {{/Taxes.length}}
                            {{#Taxes}}
                                <p>{{{Notes}}} <span class="glyphicons circle_question_mark"><a data-toggle="collapse" data-target="#description{{RelatedToID}}" class="load_desc" data-entity="tax" data-related-id="{{RelatedToID}}"><i></i></a></span><span class="pull-right">{{#SubTotal}}{{SubTotal2.LocalCurrencySymbol}}{{SubTotal2.LocalSValue2}}{{/SubTotal}}{{^SubTotal}}--{{/SubTotal}}</span></p><div id="description{{RelatedToID}}" class="collapse charges-description">{{{Description}}}</div>
                            {{/Taxes}}
                            {{#Taxes.length}}
                            </div>
                                <hr />
                            {{/Taxes.length}}
                        {{/config.statementsettings.lumptaxes}}
                        {{#OptionalFees.length}}
                        <div id="ofees-modal" class="item row-fluid">
                            <h5>{{textdata.Optional Fees & Services}}</h5>
                        {{/OptionalFees.length}}
                        {{#OptionalFees}}
                            <p>{{{Notes}}} <span class="glyphicons circle_question_mark"><a data-toggle="collapse" data-target="#description{{RelatedToID}}" class="load_desc" data-entity="fee" data-related-id="{{RelatedToID}}"><i></i></a></span><span class="pull-right">{{#SubTotal}}{{SubTotal2.LocalCurrencySymbol}}{{SubTotal2.LocalSValue2}}{{/SubTotal}}{{^SubTotal}}--{{/SubTotal}}</span></p><div id="description{{RelatedToID}}" class="collapse charges-description">{{{Description}}}</div>
                        {{/OptionalFees}}
                        {{#OptionalFees.length}}
                        </div>
                            <hr />
                        {{/OptionalFees.length}}
                        {{#Discounts.length}}
                        <div id="discounts-modal" class="item row-fluid">
                            <h5>{{textdata.Discounts}}</h5>
                        {{/Discounts.length}}
                        {{#Discounts}}
                            <p>{{{Notes}}}<span class="pull-right">{{#SubTotal}}{{SubTotal2.LocalCurrencySymbol}}{{SubTotal2.LocalSValue2}}{{/SubTotal}}{{^SubTotal}}--{{/SubTotal}}</span></p>
                        {{/Discounts}}
                        {{#Discounts.length}}
                        </div>
                            <hr />
                        {{/Discounts.length}}
                        {{#Deposits.length}}
                        <div id="deposits-modal" class="item row-fluid">
                            <h5>{{textdata.Deposits}}</h5>
                        {{/Deposits.length}}
                        {{#Deposits}}
                            <p>{{{Notes}}} <span class="glyphicons circle_question_mark"><a data-toggle="collapse" data-target="#description{{RelatedToID}}" class="load_desc" data-entity="fee" data-related-id="{{RelatedToID}}"><i></i></a></span><span class="pull-right">{{#SubTotal}}{{SubTotal2.LocalCurrencySymbol}}{{SubTotal2.LocalSValue2}}{{/SubTotal}}{{^SubTotal}}--{{/SubTotal}}</span></p><div id="description{{RelatedToID}}" class="collapse charges-description">{{{Description}}}</div>
                        {{/Deposits}}
                        {{#Deposits.length}}
                        </div>
                        {{/Deposits.length}}
                    </div>
                    <div class="modal-footer">
                        <button class="btn" data-dismiss="modal" aria-hidden="true">{{textdata.Close}}</button>
                    </div>
                </div>
            {{/ContextData.Quote.Statement}}
        {{/result}}
    </div>
    </div>
</script>

