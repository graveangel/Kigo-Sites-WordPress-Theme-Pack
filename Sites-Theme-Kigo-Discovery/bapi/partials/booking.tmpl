
<script id="tmpl-booking-makebooking-masterlayout" type="text/html">
        {{#result}}
        <div class="module shadow-border">
            <div class="row-fluid">
                <header class="pagination-centered pd">
                    <h1 class="widget_title">{{textdata.Fast Secure Online Booking}}</h1>
                    <p class="booked_prop">
                        <a href="{{&ContextData.SEO.DetailURL}}"><strong>{{Headline}}</strong></a><span class="hidden-phone">&nbsp;-&nbsp;{{Type}}
                        {{#Bedrooms}}&nbsp;/&nbsp;{{textdata.Beds}}&nbsp;{{Bedrooms}}{{/Bedrooms}}
                        {{#Bathrooms}}&nbsp;/&nbsp;{{textdata.Baths}}&nbsp;{{Bathrooms}}{{/Bathrooms}}
                        {{#Sleeps}}&nbsp;/&nbsp;{{textdata.Sleeps}}&nbsp;{{Sleeps}}{{/Sleeps}}</span>
                    </p>
                    <div class="booking-prop-img col-xs-12">
                        <a href="{{&ContextData.SEO.DetailURL}}" style="background-image: url('{{&PrimaryImage.OriginalURL}}')"></a>
                    </div>
                </header>
                <div class="booking-page bordersolid">
                    <article class="col-xs-8 booking-form">
                        <div class="form-horizontal pd">
                            <div id="renter"></div>
                            <hr />
                            <div id="creditcard"></div>
                            <hr />
                            <div id="accept"></div>
                        </div>
                    </article>

                    <aside class="property-info col-xs-4 pd">
                        <div class="row">
                            <div class="col-xs-12 pd">
                                <div id="stayinfo"></div>
                                <div id="statement"></div>
                                <div id="question-support" class="item row-fluid">
                                    <h5 class="module"><i class="glyphicons headset"></i>{{textdata.Have a Question?}}</h5>
                                    <p>{{textdata.Phone}}: <span class="pull-right">{{site.phone}}</span></p>
                                    <p>{{textdata.Email}}: <span class="pull-right">{{site.email}}</span></p>
                                </div>
                            </div>
                        </div>
                    </aside>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
        </div>
        {{/result}}
</script>

<script id="tmpl-booking-makebooking-stayinfo" type="text/html">
        {{#result}}
            {{^ContextData.Quote.IsValid}}
            <div class="alert alert-error">{{ContextData.Quote.ValidationMessage}}</div>
            {{/ContextData.Quote.IsValid}}
        <span class="module notes_2 hide">{{textdata.Booking Details}}</span>
        <div id="details" class="item row-fluid">
            <p><span class="glyphicons glyphicons-calendar m-r-15"></span><span>{{textdata.Check-In}}</span> <span class="pull-right">{{session.searchparams.scheckin}}</span></p>
            <p><span class="glyphicons glyphicons-calendar m-r-15"></span><span>{{textdata.Check-Out}}</span> <span class="pull-right">{{session.searchparams.scheckout}}</span></p>
            <p><span class="glyphicons glyphicons-group m-r-15"></span><span>{{textdata.Adults}}</span><span class="pull-right">{{session.searchparams.adults}}</span></p>
            <p><span class="glyphicons glyphicons-group m-r-15"></span><span>{{textdata.Children}}</span><span class="pull-right">{{session.searchparams.children}}</span></p>
            <p><div data-target="#revisedates" data-toggle="modal" class="bapi-showrevise kd-button secondary-fill-color filled">{{textdata.Change Info}}</div></p>
        </div>

        <div  class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-body">
                <div class="row-fluid">

                </div>
                <div class="row-fluid">
                    <div class="span6">
                        <label>{{textdata.Adults}}</label>
                        <input class="span12 revisedates" type="text" placeholder="{{textdata.Adults}}" value="{{#session.searchparams.adults.min}}{{session.searchparams.adults.min}}{{/session.searchparams.adults.min}}{{^session.searchparams.adults.min}}2{{/session.searchparams.adults.min}}" data-field="adults[min]" required>
                    </div>
                    <div class="span6">
                        <label>{{textdata.Children}}</label>
                        <input class="span12 revisedates" type="text" placeholder="{{textdata.Children}}" value="{{#session.searchparams.children.min}}{{session.searchparams.children.min}}{{/session.searchparams.children.min}}{{^session.searchparams.children.min}}0{{/session.searchparams.children.min}}" data-field="children[min]" required>
                    </div>
                </div>
            </div>
        </div>

        <div id="revisedates" data-backdrop="false" class="pop-up-booking-form modal fade" role="dialog" aria-hidden="true">

            <form class="revisedates">
                <div class="booking-form">
                    <span class="glyphicons glyphicons-list-alt"></span><span class="update-info-label">{{textdata.Change Info}}</span>
                    <div class="row-fluid">
                        <div class="span6">
                            <label>{{textdata.Check-In}}</label>
                            <input id="makebookingcheckin" class="span12 revisedates datepickercheckin" type="text" placeholder="{{textdata.Check-In}}" value="{{session.searchparams.scheckin}}" data-field="scheckin" required>
                        </div>
                        <div class="span6">
                            <label>{{textdata.Check-Out}}</label>
                            <input id="makebookingcheckout" class="span12 revisedates datepickercheckout" type="text" placeholder="{{textdata.Check-Out}}" value="{{session.searchparams.scheckout}}" data-field="scheckout" required>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-xs-6">
                            <label>Adults</label>
                            <input class="col-xs-12 revisedates" type="text" placeholder="{{textdata.Adults}}" value="{{#session.searchparams.adults}}{{session.searchparams.adults}}{{/session.searchparams.adults}}{{^session.searchparams.adults}}2{{/session.searchparams.adults}}" data-field="adults[min]" required="">
                        </div>
                        <div class="col-xs-6">
                            <label>Children</label>
                            <input class="col-xs-12 revisedates" type="text" placeholder="{{textdata.Children}}" value="{{#session.searchparams.children}}{{session.searchparams.children}}{{/session.searchparams.children}}{{^session.searchparams.children}}0{{/session.searchparams.children}}" data-field="children[min]" required="">
                        </div>
                    </div>

                    <div class="modal-footer">
                        <button class="btn" data-dismiss="modal" aria-hidden="true">{{textdata.Close}}</button>
                        <button data-dismiss="modal" class="btn btn-primary bapi-revisedates">{{textdata.Update}}</button>
                    </div>
                </div>
            </form>
        </div>

        {{/result}}
</script>

<script id="tmpl-booking-makebooking-statement" type="text/html">
    <hr />
        {{#result}}
            {{#ContextData.Quote.IsValid}}
                {{#ContextData.Quote.Statement}}
                <div id="charges" class="item row-fluid">
