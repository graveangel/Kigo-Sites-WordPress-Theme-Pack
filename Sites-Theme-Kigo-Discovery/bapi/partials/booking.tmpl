                            <p>{{textdata.You will pay in}} {{Currency}}</p>
                            <p><strong>{{textdata.Total}}<span class="pull-right">{{TotalDueNow.CurrencySymbol}} {{Total}}</span></strong></p>
                            <p><strong>{{textdata.Amount Due Now}}<span class="pull-right">{{TotalDueNow.CurrencySymbol}} {{TotalDueNow.SValue2}}</span></strong></p>
                        {{/HasLocalCurrency}}
                    </div>
                    {{#ContextData.Quote.PaymentTerms}}
