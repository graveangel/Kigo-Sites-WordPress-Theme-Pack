<script id="tmpl-booking-makebooking-renter" type="text/html">
    <legend>1. {{textdata.Guest Information}}</legend>
    <div class="row">
        <div class="control-group col-xs-6">
            <label class="control-label" for="renterfirstname">{{textdata.First Name}}:*</label>
            <div class="controls">
                <input type="text" id="renterfirstname" class="bookingfield col-xs-12 required" data-field="Renter[FirstName]" required />
            </div>
        </div>
        <div class="control-group col-xs-6">
            <label class="control-label" for="renterlastname">{{textdata.Last Name}}:*</label>
            <div class="controls">
                <input type="text" id="renterlastname" class="bookingfield col-xs-12 required" data-field="Renter[LastName]" required />
            </div>
        </div>
        <div class="control-group col-xs-6">
            <label class="control-label" for="renteremail">{{textdata.Email}}:*</label>
            <div class="controls">
                <input type="text" id="renteremail" class="bookingfield col-xs-12 required" data-validity="email" data-field="Renter[PrimaryEmail]" required />
            </div>
        </div>
        <div class="control-group col-xs-6">
            <label class="control-label" for="renterphone">{{textdata.Phone}}:*</label>
            <div class="controls">
                <input type="text" id="renterphone" class="bookingfield col-xs-12 required" data-field="Renter[HomePhone]" required />
            </div>
        </div>
        <div class="control-group col-xs-6">
            <label class="control-label" for="renterls">{{config.leadsources.prompt}}:</label>
            <div class="controls">
                <!--<div class="btn-group">-->
                <select id="renterls" class="bookingfield col-xs-12" data-field="Renter[LeadSourceID]">
                    <option value="0">-{{config.leadsources.prompt}}-</option>
                    {{#config.leadsources.values}}<option value="{{Data}}">{{Label}}</option>{{/config.leadsources.values}}
                </select>
                <!--</div>-->
            </div>
        </div>
        <div class="control-group col-xs-6 ">
            <label class="control-label" for="publicnotes">{{textdata.Special Requests}}:</label>
            <div class="controls">
                <input type="text" id="publicnotes" rows="3" class="bookingfield col-xs-12" data-field="PublicNotes" />
            </div>
        </div>
    </div>
</script>

<script id="tmpl-booking-makebooking-creditcard" type="text/html">
    <legend>
        <div class="control row">
            <span class="col-xs-8">2. {{textdata.Credit Card Information}}</span>
            <div class="credit-icon col-xs-4">
                {{#config.ccconfig.visa}}
                    <span class="creditcard-visa"></span>
                {{/config.ccconfig.visa}}
                {{#config.ccconfig.mastercard}}
                    <span class="creditcard-mastercard"></span>
                {{/config.ccconfig.mastercard}}
                {{#config.ccconfig.discover}}
                    <span class="creditcard-discover"></span>
                {{/config.ccconfig.discover}}
                {{#config.ccconfig.amex}}
                    <span class="creditcard-amex"></span>
                {{/config.ccconfig.amex}}
                {{#config.ccconfig.dinersclub}}
                    <span class="creditcard-dinersclub"></span>
                {{/config.ccconfig.dinersclub}}
            </div>
        </div>
    </legend>
    <div class="row">
        <div class="control-group col-xs-6 ">
            <label class="control-label" for="noc">{{textdata.Name on Card}}:</label>
            <div class="controls">
                <input type="text" id="noc" class="bookingfield col-xs-12 required autofullname" data-field="CreditCard[NameOnCard]" required />
            </div>
        </div>
        <div class="control-group col-xs-6 ">
            <label class="control-label" for="ccnum">{{textdata.Card Number}}:</label>
            <div class="controls">
                <input type="text" id="ccnum" class="bookingfield col-xs-12 required ccverify" data-isvalid="0" data-field="CreditCard[CardNumber]" required />
            </div>
        </div>
        <div class="control-group col-xs-6 ">
            <label class="control-label inline" for="ccexpr">{{textdata.Expiration}}:</label>
            <div class="controls row">

                <div class="col-xs-6">
                    <select id="ccmonth" class="bookingfield required col-xs-12" data-field="CreditCard[ExpiryMonth]" required>
                        <option value="">-{{textdata.Month}}-</option>
                        {{#config.ccmonths.values}}
                            <option value="{{Data}}">{{Label}}</option>
                        {{/config.ccmonths.values}}
                    </select>
                </div>

                <div class="col-xs-6">
                    <select id="ccyear" class="bookingfield required col-xs-12" data-field="CreditCard[ExpiryYear]" required>
                        <option value="">-{{textdata.Year}}-</option>
                        {{#config.ccyears.values}}
                            <option value="{{Data}}">{{Label}}</option>
                        {{/config.ccyears.values}}
                    </select>
                </div>
            </div>
        </div>
        <div class="control-group col-xs-6 ">
            <label class="control-label" for="cccvv">{{textdata.Security Code}}:</label>
            <div class="controls">
                <input type="text" id="cccvv" class="bookingfield col-xs-12 required" data-field="CreditCard[SecurityCode]" required />
            </div>
        </div>
        <hr />
        <legend class="col-xs-12">3. {{textdata.Billing Address}}</legend>
        <div class="control-group col-xs-6 ">
            <label class="control-label" for="cccvv">{{textdata.Country}}:</label>
            <div class="controls">
                <input type="text" id="billingcountry" class="bookingfield col-xs-12 required bapi-country" data-field="CreditCard[Country]" required />
            </div>
        </div>
        <div class="control-group col-xs-6 ">
            <label class="control-label" for="billingaddress">{{textdata.Street Line 1}}:</label>
            <div class="controls">
                <input type="text" id="billingaddress" class="bookingfield col-xs-12 required" data-field="CreditCard[Address]" required />
            </div>
        </div>
        <div class="control-group col-xs-6 ">
            <label class="control-label" for="billingaddress2">{{textdata.Street Line 2}}:</label>
            <div class="controls">
                <input type="text" id="billingaddress2" class="bookingfield col-xs-12" data-field="CreditCard[Address2]" />
            </div>
        </div>
        <div class="control-group col-xs-6 ">
            <label class="control-label" for="billingcity">{{textdata.City}}/{{textdata.Town}}:</label>
            <div class="controls">
                <input type="text" id="billingcity" class="bookingfield col-xs-12 required bapi-city" data-field="CreditCard[City]" required />
            </div>
        </div>
        <div class="control-group col-xs-6">
            <div class="row">
                <div class="col-xs-6">
                    <label class="control-label inline" for="billingstate">{{textdata.State}}/{{textdata.Province}}:</label>
                    <div class="controls">
                        <input type="text" id="billingstate" class="col-xs-12 bookingfieldrequired bapi-state" data-field="CreditCard[State]" required />
                    </div>
                </div>

