/*
Group: Properties
*/
<script id="tmpl-propertysearch-listview" type="text/html">
        {{#result}}
        <div class="col-md-6">
            <div class="property">
                <a href="{{&ContextData.SEO.DetailURL}}" class="image" style="background-image: url('{{&PrimaryImage.ThumbnailURL}}')"></a>
                <div class="info">
                    <div class="top">
                        <div class="beds">
                            {{#Bedrooms}}{{Bedrooms}} {{textdata.Beds}}{{/Bedrooms}} {{#Bathrooms}} /  {{Bathrooms}} {{textdata.Baths}}{{/Bathrooms}}
                        </div>
                        <div class="location">
                            {{Type}}, {{{City}}}<br>
                        </div>
                    </div>
                    <h5 class="title">{{Headline}}</h5>
                    <div class="desc">{{{ContextData.SEO.PageTitle}}}</div>

                    <div class="bottom">
                        <div class="price">{{ContextData.Quote.PublicNotes}}</div>
                        <a href="{{&ContextData.SEO.DetailURL}}" class="seeDetails">See Details</a>
                    </div>
                </div>
            </div>
        </div>
        {{/result}}
        {{^result}}
            <div class="nomore"><a href='javascript:void(0)'><span class="col-xs-12 alert alert-warn">{{textdata.There are no more results}}</span></a></div>
        {{/result}}
</script>

<script id="tmpl-propertysearch-mapview" type="text/html">
        {{#result}}
        <div class="property col-md-6" data-altid='{{AltID}}'>
            <div class="viewInMap"></div>
            <a href="{{&ContextData.SEO.DetailURL}}" class="image" style="background-image: url('{{&PrimaryImage.ThumbnailURL}}')">
                <div class="from secondary-fill-color">
                    <!--<div class="tag">From:</div>-->
                    <div class="price">{{ContextData.Quote.PublicNotes}}</div>
                </div>
            </a>
            <div class="info">
                <h5 class="title">{{Headline}}</h5>
                {{Type}}, {{{Location}}}<br>
                {{#Bedrooms}}{{textdata.Beds}} {{Bedrooms}}{{/Bedrooms}} {{#Bathrooms}} / {{textdata.Baths}} {{Bathrooms}}{{/Bathrooms}}
                <i class="kd-icon-right_arrow"></i>
            </div>
        </div>
        {{/result}}
        {{^result}}
        <div class="nomore"><a href='javascript:void(0)'><span class="col-xs-12 alert alert-warn">{{textdata.There are no more results}}</span></a></div>
        {{/result}}
</script>
