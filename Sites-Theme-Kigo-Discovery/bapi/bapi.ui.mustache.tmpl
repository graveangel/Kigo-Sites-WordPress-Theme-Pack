                                                        {{/ContextData.SEO.DetailURL}}
                                                        <img src='{{&PrimaryImage.ThumbnailURL}}' alt='{{PrimaryImage.Caption}}' title='{{PrimaryImage.Caption}}' />
                                                        {{#ContextData.SEO.DetailURL}}
                                                    </a>
                                                    {{/ContextData.SEO.DetailURL}}
                                                </div>
                                                <div class="gmap-text">
                                                    <p>{{#ContextData.SEO.DetailURL}}<a href="{{ContextData.SEO.DetailURL}}">{{/ContextData.SEO.DetailURL}}<b>{{Headline}}</b>{{#ContextData.SEO.DetailURL}}</a>{{/ContextData.SEO.DetailURL}}</p>

