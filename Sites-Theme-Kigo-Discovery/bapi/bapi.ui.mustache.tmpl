    {{/ContextData.Quote.IsValid}}
    <span class="glyphicons module notes_2 hide"><i></i>{{textdata.Booking Details}}</span>
    <div id="details" class="item row-fluid">
        <p><span class="glyphicons calendar"><i></i>{{textdata.Check-In}}</span> <span class="pull-right">{{session.searchparams.scheckin}}</span></p>
        <p><span class="glyphicons calendar"><i></i>{{textdata.Check-Out}}</span> <span class="pull-right">{{session.searchparams.scheckout}}</span></p>
