        <p><span class="glyphicons group"><i></i>{{textdata.Children}}</span><span class="pull-right">{{session.searchparams.children.min}}</span></p>
        <p><a href="#revisedates" data-toggle="modal" class="bapi-showrevise kd-button filled">{{textdata.Change Info}}</a></p>
    </div>

    <div id="revisedates" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-body">
            <div class="row-fluid">
                <div class="col-xs-6">
                    <label>{{textdata.Check-In}}</label>
                    <input id="makebookingcheckin" class="col-xs-12 revisedates datepickercheckin" type="text" placeholder="{{textdata.Check-In}}" value="{{session.searchparams.scheckin}}" data-field="scheckin" required>
                </div>
                <div class="col-xs-6">
                    <label>{{textdata.Check-Out}}</label>
                    <input id="makebookingcheckout" class="col-xs-12 revisedates datepickercheckout" type="text" placeholder="{{textdata.Check-Out}}" value="{{session.searchparams.scheckout}}" data-field="scheckout" required>
                </div>
            </div>
            <div class="row-fluid">
                <div class="col-xs-6">
                    <label>{{textdata.Adults}}</label>
                    <input class="col-xs-12 revisedates" type="text" placeholder="{{textdata.Adults}}" value="{{#session.searchparams.adults.min}}{{session.searchparams.adults.min}}{{/session.searchparams.adults.min}}{{^session.searchparams.adults.min}}2{{/session.searchparams.adults.min}}" data-field="adults[min]" required>
                </div>
                <div class="col-xs-6">
                    <label>{{textdata.Children}}</label>
                    <input class="col-xs-12 revisedates" type="text" placeholder="{{textdata.Children}}" value="{{#session.searchparams.children.min}}{{session.searchparams.children.min}}{{/session.searchparams.children.min}}{{^session.searchparams.children.min}}0{{/session.searchparams.children.min}}" data-field="children[min]" required>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button class="btn" data-dismiss="modal" aria-hidden="true">{{textdata.Close}}</button>
            <button class="btn btn-primary bapi-revisedates">{{textdata.Update}}</button>
        </div>
    </div>
    {{/result}}
</script>

